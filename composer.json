{
    "name": "mccaulay/duskless",
    "description": "Laravel Duskless provides browser automation.",
    "keywords": [
        "laravel",
        "automation",
        "webdriver"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "ext-json": "*",
        "ext-zip": "*",
        "facebook/webdriver": "^1.7",
        "nesbot/carbon": "^1.20|^2.0",
        "illuminate/console": "~5.8.0|^6.0",
        "illuminate/support": "~5.8.0|^6.0",
        "symfony/console": "^4.0",
        "symfony/finder": "^4.0",
        "symfony/process": "^4.0",
        "vlucas/phpdotenv": "^2.2|^3.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "McCaulay\\Duskless\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "McCaulay\\Duskless\\DusklessServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}